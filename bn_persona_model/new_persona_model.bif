%VARIABLES DEFINITION
variable reactivity {
  type discrete [3] {slow, medium, fast};
}
variable memory {
  type discrete[3] {low, medium, high};
}
variable attention {
  type discrete[3] {low, medium, high};
}
variable robot_assistance {
  type discrete [ 5 ] { lev_0, lev_1, lev_2, lev_3, lev_4 };
}
variable attempt {
  type discrete [ 4 ] { att_1, att_2, att_3, att_4 };
}
variable game_state {
